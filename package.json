{
  "name": "farbodsz.net",
  "private": true,
  "description": "Personal website",
  "version": "0.2.0",
  "scripts": {
    "build": "NODE_OPTIONS=--openssl-legacy-provider gatsby build",
    "develop": "gatsby develop",
